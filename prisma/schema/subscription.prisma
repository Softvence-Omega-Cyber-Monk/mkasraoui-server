model Subscription {
    id String @id @default(uuid())

    plan_name Plan_name
    status Subscription_status
    start_data DateTime
    end_date DateTime?

    plan_name String
    status Subscription_status
    start_data DateTime
    end_date DateTime

    stripe_subscription_id String?
    user_id String
